{
	"name": "PersonalDashboard",
	"author": [
		"Jason Sherman",
		"[https://www.mediawiki.org/wiki/Growth/Team WMF Growth Team]"
	],
	"url": "https://www.mediawiki.org/wiki/Special:MyLanguage/Extension:PersonalDashboard",
	"descriptionmsg": "personal-dashboard-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.45",
		"extensions": {
			"CommunityConfiguration": "*"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\PersonalDashboard\\": "src/"
	},
	"ServiceWiringFiles": [
		"ServiceWiring.php"
	],
	"SpecialPages": {
		"PersonalDashboard": "MediaWiki\\Extension\\PersonalDashboard\\Specials\\SpecialPersonalDashboard"
	},
	"ConfigRegistry": {
		"PersonalDashboard": "MediaWiki\\Config\\GlobalVarConfig::newInstance"
	},
	"HookHandlers": {
		"skintemplatenav-universal": {
			"class": "MediaWiki\\Extension\\PersonalDashboard\\HookHandler\\SkinTemplateNavigationUniversalHandler",
			"services": [ "SpecialPageFactory" ]
		},
		"special-page": {
			"class": "MediaWiki\\Extension\\PersonalDashboard\\HookHandler\\SpecialPageInitListHandler"
		}
	},
	"Hooks": {
		"CommunityConfigurationSchemaBeforeEditor": "communityconfig",
		"CommunityConfigurationProvider_initList": "communityconfig",
		"MediaWikiServices": "mediawikiservices",
		"MessageCacheFetchOverrides": "earlylifecycle",
		"ParserFirstCallInit": "parserfunctions",
		"SkinTemplateNavigation::Universal": "skintemplatenav-universal",
		"SkinMinervaOptionsInit": [
			"MediaWiki\\Extension\\PersonalDashboard\\HookHandler::onSkinMinervaOptionsInit"
		],
		"SpecialPage_initList": [
			"config",
			"special-page"
		],
		"UserSendConfirmationMail": "confirmEmail"
	},
	"attributes": {
		"CommunityConfiguration": {
			"Providers": {
				"PersonalDashboardRiskyArticleEdits": {
					"store": {
						"type": "wikipage",
						"args": [
							"MediaWiki:PersonalDashboardRiskyArticleEdits.json"
						]
					},
					"validator": {
						"type": "jsonschema",
						"args": [
							"MediaWiki\\Extension\\PersonalDashboard\\Config\\Schemas\\RiskyArticleEditsSchema"
						]
					},
					"type": "mw-config",
					"options": {
						"helpPage": "Help:PersonalDashboard/RiskyArticleEdits"
					}
				}
			}
		}
	},

	"MessagesDirs": {
		"PersonalDashboard": [
			"i18n",
			"i18n/ext.personalDashboard.impact",
			"i18n/ext.personalDashboard.policiesGuidelines",
			"i18n/ext.personalDashboard.riskyArticleEdits",
			"i18n/ext.personalDashboard.riskyArticleEdits.mobile"
		]
	},
	"ExtensionMessagesFiles": {
		"PersonalDashboardAlias": "PersonalDashboard.i18n.alias.php"
	},
	"ResourceModules": {
		"ext.personalDashboard": {
			"localBasePath": "resources/ext.personalDashboard",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard",
			"dependencies": [
				"mediawiki.user",
				"oojs-ui-core",
				"oojs-ui-windows"
			],
			"styles": [],
			"packageFiles": [
				"init.js"
			],
			"messages": []
		},
		"ext.personalDashboard.common": {
			"localBasePath": "resources/ext.personalDashboard.common",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard.common",
			"dependencies": [
				"vue"
			],
			"packageFiles": [
				"composables/useFetchActivityResult.js",
				"index.js"
			]
		},
		"ext.personalDashboard.menuIcon": {
			"class": "MediaWiki\\ResourceLoader\\OOUIIconPackModule",
			"selectorWithVariant": ".vector-icon.mw-ui-icon-wikimedia-{name}-{variant}",
			"selectorWithoutVariant": ".vector-icon.mw-ui-icon-wikimedia-{name}, .minerva-icon--{name}",
			"useMaskImage": true,
			"icons": [ "viewCompact" ]
		},
		"ext.personalDashboard.impact": {
			"localBasePath": "resources/ext.personalDashboard.impact",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard.impact",
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"codexComponents": [
				"CdxButton",
				"CdxIcon",
				"CdxPopover"
			],
			"dependencies": [
				"vue"
			],
			"messages": [
				"personal-dashboard-impact-thanks-sent",
				"personal-dashboard-impact-edits-reviewed",
				"personal-dashboard-impact-edits-reviewed-description",
				"personal-dashboard-impact-info-button"
			],
			"packageFiles": [
				"init.js",
				"App.vue",
				{
					"name": "icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconCheckAll",
						"cdxIconInfo",
						"cdxIconInfoFilled",
						"cdxIconUserTalk"
					]
				}
			]
		},
		"ext.personalDashboard.policiesGuidelines": {
			"localBasePath": "resources/ext.personalDashboard.policiesGuidelines",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard.policiesGuidelines",
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"codexComponents": [
				"CdxButton",
				"CdxCard",
				"CdxDialog",
				"CdxIcon"
			],
			"dependencies": [
				"vue"
			],
			"messages": [
				"personal-dashboard-policies-guidelines-title",
				"personal-dashboard-policies-guidelines-body",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-title",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-definition",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-example-1",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-answer-1-label",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-answer-1-text",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-example-2",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-answer-2-label",
				"personal-dashboard-policies-guidelines-neutral-point-of-view-answer-2-text",
				"personal-dashboard-policies-guidelines-no-original-research-title",
				"personal-dashboard-policies-guidelines-no-original-research-definition",
				"personal-dashboard-policies-guidelines-no-original-research-example-1",
				"personal-dashboard-policies-guidelines-no-original-research-answer-1-label",
				"personal-dashboard-policies-guidelines-no-original-research-answer-1-text",
				"personal-dashboard-policies-guidelines-no-original-research-example-2",
				"personal-dashboard-policies-guidelines-no-original-research-answer-2-label",
				"personal-dashboard-policies-guidelines-no-original-research-answer-2-text",
				"personal-dashboard-policies-guidelines-verifiability-title",
				"personal-dashboard-policies-guidelines-verifiability-definition",
				"personal-dashboard-policies-guidelines-verifiability-example-1",
				"personal-dashboard-policies-guidelines-verifiability-answer-1-label",
				"personal-dashboard-policies-guidelines-verifiability-answer-1-text",
				"personal-dashboard-policies-guidelines-verifiability-example-2",
				"personal-dashboard-policies-guidelines-verifiability-answer-2-label",
				"personal-dashboard-policies-guidelines-verifiability-answer-2-text",
				"personal-dashboard-policies-guidelines-verifiability-example-3",
				"personal-dashboard-policies-guidelines-verifiability-answer-3-label",
				"personal-dashboard-policies-guidelines-verifiability-answer-3-text",
				"personal-dashboard-policies-guidelines-assume-good-faith-title",
				"personal-dashboard-policies-guidelines-assume-good-faith-definition",
				"personal-dashboard-policies-guidelines-assume-good-faith-example-1",
				"personal-dashboard-policies-guidelines-assume-good-faith-answer-1-label",
				"personal-dashboard-policies-guidelines-assume-good-faith-answer-1-text",
				"personal-dashboard-policies-guidelines-assume-good-faith-example-2",
				"personal-dashboard-policies-guidelines-assume-good-faith-answer-2-label",
				"personal-dashboard-policies-guidelines-assume-good-faith-answer-2-text",
				"personal-dashboard-policies-guidelines-examples-button",
				"personal-dashboard-policies-guidelines-examples-progress",
				"personal-dashboard-policies-guidelines-examples-header",
				"personal-dashboard-policies-guidelines-next-button",
				"personal-dashboard-policies-guidelines-previous-button",
				"personal-dashboard-policies-guidelines-got-it-button"
			],
			"packageFiles": [
				"init.js",
				"App.vue",
				"components/ExamplesDialog.vue",
				"components/ListCard.vue",
				{
					"name": "icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconAlert",
						"cdxIconError",
						"cdxIconNext",
						"cdxIconPrevious",
						"cdxIconSuccess"
					]
				}
			]
		},
		"ext.personalDashboard.riskyArticleEdits": {
			"localBasePath": "resources/ext.personalDashboard.riskyArticleEdits",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard.riskyArticleEdits",
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"codexComponents": [
				"CdxCard",
				"CdxIcon",
				"CdxPopover",
				"CdxToggleButton"
			],
			"packageFiles": [
				"init.js",
				"App.vue",
				"components/ListCard.vue",
				"components/RecentActivity.vue",
				"components/ChangeNumber.vue",
				"components/InfoPopover.vue",
				"components/CreatorByline.vue",
				{
					"name": "icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconArticle",
						"cdxIconUserActive",
						"cdxIconTextSummary",
						"cdxIconInfoFilled"
					]
				}
			],
			"messages": [
				"personal-dashboard-risky-article-edits-header",
				"personal-dashboard-risky-article-edits-subheader",
				"personal-dashboard-risky-article-edits-footer-preamble",
				"personal-dashboard-risky-article-edits-info-title",
				"personal-dashboard-risky-article-edits-info-article-title",
				"personal-dashboard-risky-article-edits-info-author",
				"personal-dashboard-risky-article-edits-info-time-text-today",
				"personal-dashboard-risky-article-edits-info-edit-summary",
				"personal-dashboard-risky-article-edits-info-bytes",
				"personal-dashboard-risky-article-edits-info-subtitle",
				"personal-dashboard-risky-article-edits-list-card-no-comment-message",
				"tag-mw-reverted",
				"tag-mw-rollback",
				"tag-mw-replace",
				"tag-mw-manual-revert"

			],
			"dependencies": [
				"vue",
				"mediawiki.DateFormatter",
				"ext.personalDashboard.common"
			]
		},
		"ext.personalDashboard.riskyArticleEdits.mobile": {
			"localBasePath": "resources/ext.personalDashboard.riskyArticleEdits.mobile",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard.riskyArticleEdits.mobile",
			"class": "MediaWiki\\ResourceLoader\\CodexModule",
			"codexComponents": [
				"CdxButton",
				"CdxCard",
				"CdxIcon"
			],
			"packageFiles": [
				"init.js",
				"App.vue",
				"components/ListCard.vue",
				"components/RecentActivity.vue",
				"components/ChangeNumber.vue",
				{
					"name": "icons.json",
					"callback": "MediaWiki\\ResourceLoader\\CodexModule::getIcons",
					"callbackParam": [
						"cdxIconEdit"
					]
				}
			],
			"messages": [
				"personal-dashboard-risky-article-edits-mobile-summary-header",
				"personal-dashboard-risky-article-edits-mobile-summary-footer-link-text",
				"personal-dashboard-risky-article-edits-mobile-byte-text",
				"personal-dashboard-risky-article-edits-mobile-user-text"
			],
			"dependencies": [
				"vue",
				"mediawiki.DateFormatter",
				"ext.personalDashboard.common"
			]
		},
		"ext.personalDashboard.styles": {
			"localBasePath": "resources/ext.personalDashboard.styles",
			"remoteExtPath": "PersonalDashboard/resources/personalDashboard.styles",
			"styles": [
				"BaseModule.less",
				"BannerModule.less",
				"PersonalDashboard.less",
				"../vue-components/Skeleton.less"
			]
		},
		"ext.personalDashboard.icons": {
			"class": "MediaWiki\\ResourceLoader\\ImageModule",
			"localBasePath": "resources/images",
			"selectorWithVariant": ".oo-ui-image-{variant}.oo-ui-icon-{name}",
			"selectorWithoutVariant": ".oo-ui-icon-{name},.mw-ui-icon-{name}",
			"variants": {
				"progressive": {
					"color": "#3366cc"
				}
			},
			"images": {
				"personal-dashboard-previous": {
					"file": {
						"ltr": "arrowPrevious-ltr.svg",
						"rtl": "arrowPrevious-rtl.svg"
					}
				}
			}
		}
	},
	"config": {
		"PersonalDashboardDeveloperSetup": {
			"description": "Catch fewer errors for easier troubleshooting",
			"value": false
		},
		"PersonalDashboardVariant": {
			"description": "The dashboard variant served to users",
			"value": null
		}
	},
	"DatabaseVirtualDomains": [
		"virtual-dashboard"
	],
	"manifest_version": 2
}
