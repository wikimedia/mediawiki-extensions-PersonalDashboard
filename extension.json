	"config": {
		"PersonalDashboardDeveloperSetup": {
			"description": "Catch fewer errors for easier troubleshooting",
			"value": false
		},
		"PersonalDashboardVariant": {
			"description": "The dashboard variant served to users",
			"value": null
		},
		"PersonalDashboardSurveyLink": {
			"description": "External link to the Personal Dashboard feedback survey.",
			"value": null
		},
		"PersonalDashboardBlueDot": {
			"description": "Show a blue dot inviting users to try out the dashboard.",
			"value": true
		},
		"PersonalDashboardBlueDotMinimumEdits": {
			"description": "The minimum number of edits required to show the blue dot.",
			"value": 100
		}
	},
	"DatabaseVirtualDomains": [
		"virtual-dashboard"
	],
	"manifest_version": 2
}
